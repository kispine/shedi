{
  "root": "apps/webclient",
  "sourceRoot": "apps/webclient",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "root": "apps/webclient",
        "outputPath": "dist/apps/webclient"
      },
      "configurations": {
        "production": {}
      }
    },
    "serve": {
      "executor": "@nrwl/next:server",
      "options": {
        "buildTarget": "webclient:build",
        "dev": true,
        "port": 12000,
        "proxyConfig": "apps/api/proxy.conf.json"
      },
      "configurations": {
        "production": {
          "buildTarget": "webclient:build:production",
          "dev": false
        }
      }
    },
    "export": {
      "executor": "@nrwl/next:export",
      "options": {
        "buildTarget": "webclient:build:production"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/webclient"],
      "options": {
        "jestConfig": "apps/webclient/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/webclient/**/*.{ts,tsx,js,jsx}"]
      }
    }
  },
  "tags": [
    "scope:webclient",
    "type:app"
  ]
}
